[core]
	excludesfile = ~/.gitignore_global
[user]
	name = Benjamin Althues
	email = benjamin@althu.es
[alias]
	st = status
	ci = commit
	ca = commit --amend --reset-author
	co = checkout
	di = diff
	br = branch
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[push]
	default = simple
