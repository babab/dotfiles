URxvt*loginShell:true
URxvt*scrollBar: off
/* URxvt.font: xft:Liberation Mono:weight=medium:pixelsize=13 */
/* URxvt.font: xft:Cosmic Sans Neue Mono:weight=medium:pixelsize=17 */
URxvt.font: xft:Fantasque Sans Mono:weight=medium:pixelsize=15
URxvt*colorMode: on
URxvt*boldColors: on
URxvt*dynamicColors: on
URxvt*colorULMode: on
URxvt*colorUL: red
URxvt*colorBDMode: on
URxvt*colorBD: white
URxvt*background: #000000
URxvt*foreground: #CC9A00
URxvt.cursorColor:  #c53525
URxvt.pointerColor: #c52325
URxvt*color0: #000000
URxvt*color1: #ca3626
URxvt*color2: #0dd743
URxvt*color3: #aed726
URxvt*color4: #2a19e4
URxvt*color5: #ca19c8
URxvt*color6: #0dbbe4
URxvt*color7: #e2e2e2
URxvt*color8: #808080
URxvt*color9: #d9574b
URxvt*color10: #32e564
URxvt*color11: #c0e54b
URxvt*color12: #4a3ef2
URxvt*color13: #d93ed9
URxvt*color14: #32cdf2
URxvt*color15: #ffffff

URxvt*termName: rxvt-unicode-256color
URxvt*visualBell: false

URxvt.perl-lib:        /usr/lib/urxvt/perl/
URxvt.urlLauncher:     firefox
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.matcher.button:  1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next

URxvt.perl-ext-common: default,url-select,keyboard-select,modeline
URxvt.modeline.fg: 12
/* URxvt.modeline.bg: 18 */
URxvt.modeline.command: urxvt-modeline

XTerm*loginShell:true
XTerm*scrollBar: off
XTerm*faceName: Liberation Mono
